class addi  
{
 int a,b,sum=0;
 void accept()
	{
 a=23;
 b=34;
 }
 void swap()
	{
 a=a+b;
 b=a-b;
 a=a-b;
 }
 void display()
	{
 System.out.println("values after swaping are:");
 System.out.println("1st value:"+a);
 System.out.println("2nd value:"+b);
 
 }
 public static void main(String args[])
	{
 addi a=new addi();
 a.accept();
 a.swap();
 a.display();
 
 }}